  "build": {
    "appId": "io.lisk.hub",
    "productName": "Lisk Hub",
    "protocols": {
      "name": "lisk-hub",
      "schemes": [
        "lisk"
      ]
    },
    "artifactName": "lisk-hub-${os}-${arch}-${version}.${ext}",
    "publish": [
      "github"
    ],
    "linux": {
      "target": [
        "AppImage"
      ],
      "desktop": {
        "MimeType": "application/lisk;x-scheme-handler/lisk"
      },
      "category": "Network"
    },
    "win": {
      "target": "nsis"
    }
  },
  "bundlesize": [
    {
      "path": "./app/build/bundle.app.*.js",
      "maxSize": "1.60 MB"
    },
    {
      "path": "./app/build/bundle.vendor.*.js",
      "maxSize": "150 kB"
    },
    {
      "path": "./app/build/styles.*.css",
      "maxSize": "1.50 MB"
    }
  ],
  "greenkeeper": {
    "ignore": [
      "postcss-cssnext"
    ]
  },
  "snyk": true,
  "engines": {
    "node": ">=v8.14.0 <v10.0.0"
  }
}
